<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:DinnerAndLove.Client.Wpf.Views"
                    xmlns:viewModel="clr-namespace:DinnerAndLove.Client.Wpf.ViewModels">

    <DataTemplate DataType="{x:Type viewModel:LoginViewModel}">
        <view:LoginView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:ProfileContentViewModel}">
        <view:ProfileContentView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:SearchDinnerViewModel}">
        <view:SearchDinnerView/>
    </DataTemplate>
</ResourceDictionary>